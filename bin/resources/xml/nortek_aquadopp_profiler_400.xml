                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="backupPeriod">
                  <swe:Quantity>
                     <swe:description>Backup in absence of power</swe:description>
                     <swe:uom code="weeks"/>
                     <swe:value>4</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="communicationParameters">
            <swe:DataRecord>

               <swe:field name="communicationType">
                  <swe:Category>
                     <swe:constraint>
                        <swe:AllowedValues>
                           <swe:value>RS-232</swe:value>
