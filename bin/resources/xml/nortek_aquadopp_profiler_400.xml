                  </swe:Quantity>
               </swe:field>

               <swe:field name="mode">
                  <swe:Category>
                     <swe:constraint>
                        <swe:AllowedValues>
                           <swe:value>Stop when full (default)</swe:value>
                           <swe:value>wrap</swe:value>
                        </swe:AllowedValues>
                     </swe:constraint>
                  </swe:Category>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="realTimeClockParameters">
            <swe:DataRecord>

               <swe:field name="accuracy">
                  <swe:QuantityRange>
                     <swe:uom code="min/year"/>
