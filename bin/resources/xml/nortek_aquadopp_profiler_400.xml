                  <swe:Quantity>
                     <swe:description>approximate length</swe:description>
                     <swe:uom code="mm"/>
                     <swe:value>600</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="maximumLengthDoubleBattery">
                  <swe:Quantity>
                     <swe:description>depending on head configuration</swe:description>
                     <swe:uom code="mm"/>
                     <swe:value>110</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="weightParameters">
            <swe:DataRecord>

               <swe:field name="weightInAir">
                  <swe:Quantity>
                     <swe:uom code="kg"/>
                     <swe:value>3.4</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="weightInWater">
                  <swe:Quantity>
                     <swe:uom code="kg"/>
                     <swe:value>0.2</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

      </sml:ParameterList>
   </sml:parameters>

</sml:PhysicalSystem>
