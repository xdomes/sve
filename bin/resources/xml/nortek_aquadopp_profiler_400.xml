                        <swe:AllowedValues>
                           <swe:value>600</swe:value>
                           <swe:value>1200</swe:value>
                        </swe:AllowedValues>
                     </swe:constraint>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

