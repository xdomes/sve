   <!--                  Profiler Outputs                  -->
   <!-- ================================================== -->

   <sml:outputs>
      <sml:OutputList>
         <sml:output name="ADCP_outputs">
            <swe:DataRecord>
               <swe:field name="pressure">
                  <swe:Quantity definition="http://mmisw.org/ont/mvco/properties/seaWaterPressure">
                     <swe:uom code="dbar"/>
                  </swe:Quantity>
               </swe:field>
               <swe:field name="temperature">
                  <swe:Quantity definition="http://mmisw.org/ont/mvco/properties/seaWaterTemperature">
                     <swe:uom code="cel"/>
                  </swe:Quantity>
               </swe:field>
               <swe:field name="velocity">
                  <swe:Quantity definition="http://mmisw.org/ont/mvco/properties/waveVelocity">
                     <swe:uom code="mm/s"/>
                  </swe:Quantity>
               </swe:field>
               <swe:field name="beamIntensity">
                  <swe:Quantity definition="http://mmisw.org/ont/mvco/properties/beamIntensity">
                     <swe:uom code="cd"/>
                  </swe:Quantity>
               </swe:field>
               <swe:field name="correlationCoefficient">
                  <swe:Quantity definition="http://sensorml.com/ont/swe/property/correlationCoefficient">
                     <swe:uom code="1"/>
                  </swe:Quantity>
               </swe:field>
            </swe:DataRecord>
         </sml:output>
      </sml:OutputList>
   </sml:outputs>

   <!-- ================================================== -->
   <!--                 Profiler Parameters                -->
   <!-- ================================================== -->

   <sml:parameters>
      <sml:ParameterList>
         <sml:parameter name="waterVelocityParameters">
            <swe:DataRecord>

               <swe:field name="maximumProfilingRange">
                  <swe:QuantityRange>
                     <swe:uom code="m"/>
                     <swe:value>60 90</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="cellSize">
                  <swe:QuantityRange>
                     <swe:uom code="m"/>
                     <swe:value>2 8</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="minimumBlanking">
                  <swe:Quantity>
                     <swe:uom code="m"/>
                     <swe:value>1</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="maximumNumberOfCells">
                  <swe:Quantity>
                     <swe:value>128</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="velocityRange">
                  <swe:QuantityRange>
                     <swe:uom code="m/s"/>
                     <swe:value>-10 10</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="accuracy">
                  <swe:QuantityRange>
                     <swe:uom code="cm/s"/>
                     <swe:value>1% of measured value plus or minus 0.5</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="maximumSamplingRate">
                  <swe:Quantity>
                     <swe:uom code="Hz"/>
                     <swe:value>1</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="internalSamplingRate">
                  <swe:Quantity>
                     <swe:uom code="Hz"/>
                     <swe:value>2</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="echoIntensityParameters">
            <swe:DataRecord>

               <swe:field name="maximumSamplingRate">
                  <swe:Quantity>
                     <swe:uom code="Hz"/>
                     <swe:value>1</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="internalSamplingRate">
                  <swe:Quantity>
                     <swe:uom code="Hz"/>
                     <swe:value>2</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="resolution">
                  <swe:Quantity>
                     <swe:uom code="dB"/>
                     <swe:value>0.45</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="dynamicRange">
                  <swe:Quantity>
                     <swe:uom code="dB"/>
                     <swe:value>90</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="transducerAcousticFrequency">
                  <swe:Quantity>
                     <swe:uom code="kHz"/>
                     <swe:value>400</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="numberOfBeams">
                  <swe:Quantity>
                     <swe:value>3</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="beamWidth">
                  <swe:Quantity>
                     <swe:uom code="degrees"/>
                     <swe:value>3.7</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="temperatureSensorParameters">
            <swe:DataRecord>

               <swe:field name="temperatureSensorType">
                  <swe:Category>
                     <swe:value>Thermistor embedded in head</swe:value>
                  </swe:Category>
               </swe:field>

               <swe:field name="temperatureRange">
                  <swe:QuantityRange>
                     <swe:uom code="deg C"/>
                     <swe:value>-4 40</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="temperatureAccuracy">
                  <swe:Quantity>
                     <swe:uom code="deg C"/>
                     <swe:value>0.1</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="temperatureResolution">
                  <swe:Quantity>
                     <swe:uom code="deg C"/>
                     <swe:value>0.01</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="temperatureTimeResponse">
                  <swe:Quantity>
                     <swe:uom code="min"/>
                     <swe:value>10</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="compassParameters">
            <swe:DataRecord>

               <swe:field name="compassType">
                  <swe:Category>
                     <swe:value>Magnetometer</swe:value>
                  </swe:Category>
               </swe:field>

               <swe:field name="compassAccuracy">
                  <swe:Quantity>
                     <swe:uom code="degrees"/>
                     <swe:value>2</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="compassResolution">
                  <swe:Quantity>
                     <swe:description>compass resolution for tilt less than 20 degrees</swe:description>
                     <swe:uom code="degree"/>
                     <swe:value>0.1</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="tiltSensorParameters">
            <swe:DataRecord>

               <swe:field name="tiltSensorType">
                  <swe:Category>
                     <swe:value>Liquid level</swe:value>
                  </swe:Category>
               </swe:field>

               <swe:field name="tiltAccuracy">
                  <swe:Quantity>
                     <swe:uom code="degrees"/>
                     <swe:value>0.2</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="tiltResolution">
                  <swe:Quantity>
                     <swe:uom code="degrees"/>
                     <swe:value>0.1</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="maximumTilt">
                  <swe:Quantity>
                     <swe:uom code="degrees"/>
                     <swe:value>30</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="upOrDown">
                  <swe:Category>
                     <swe:value>Automatic detect</swe:value>
                  </swe:Category>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="pressureSensorParameters">
            <swe:DataRecord>

               <swe:field name="pressureSensorType">
                  <swe:Category>
                     <swe:value>Piezoresistive</swe:value>
                  </swe:Category>
               </swe:field>

               <swe:field name="pressureSensorRange">
                  <swe:QuantityRange>
                     <swe:uom code="m"/>
                     <swe:value>0 100</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="pressureAccuracy">
                  <swe:Quantity>
                     <swe:uom code="% FS"/>
                     <swe:value>0.5</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="pressurePrecision">
                  <swe:Quantity>
                     <swe:uom code="% of full scale"/>
                     <swe:value>0.005</swe:value>
                  </swe:Quantity>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="analogInputParameters">
            <swe:DataRecord>

               <swe:field name="numberOfChannels">
                  <swe:Quantity>
                     <swe:value>2</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="supplyVoltage">
                  <swe:Category>
                     <swe:description>Supply voltage to analog output devices</swe:description>
                     <swe:uom code="V mA"/>
                     <swe:constraint>
                        <swe:AllowedValues>
                           <swe:value>Battery voltage/500 mA</swe:value>
                           <swe:value>+5 V/250 mA</swe:value>
                           <swe:value>+12 V/100 mA</swe:value>
                        </swe:AllowedValues>
                     </swe:constraint>
                  </swe:Category>
               </swe:field>

               <swe:field name="voltageInput">
                  <swe:QuantityRange>
                     <swe:uom code="V"/>
                     <swe:value>0 5</swe:value>
                  </swe:QuantityRange>
               </swe:field>

               <swe:field name="resolution">
                  <swe:Category>
                     <swe:value>16-bit A/D</swe:value>
                  </swe:Category>
               </swe:field>

            </swe:DataRecord>
         </sml:parameter>

         <sml:parameter name="dataRecordingParameters">
            <swe:DataRecord>

               <swe:field name="capacity">
                  <swe:Quantity>
                     <swe:uom code="MB"/>
                     <swe:value>9</swe:value>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="additionalCapacity">
                  <swe:Quantity>
                     <swe:uom code="GB"/>
                     <swe:constraint>
                        <swe:AllowedValues>
                           <swe:value>4</swe:value>
                           <swe:value>16</swe:value>
                        </swe:AllowedValues>
                     </swe:constraint>
                  </swe:Quantity>
               </swe:field>

               <swe:field name="dataRecord">
                  <swe:Quantity>
                     <swe:uom code="bytes"/>
